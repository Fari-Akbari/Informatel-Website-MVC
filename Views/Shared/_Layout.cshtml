<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    @RenderSection("HeadMeta", required: true)
    <title>@ViewBag.Title</title>
</head>
<body>
    <noscript>
        <p class="javascriptrequired">You seem to have Javascript disabled. this website needs javascript in order to function properly.</p>
    </noscript>
    <div id="wrap">
        <div id="header">
            <!-- /// HEADER  //////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
            <div class="row">
                <div class="span3" style="display: inline-block;">
                    <!-- // Logo // -->
                    <a href="@Url.Content("/home")" id="logo">
                        <img src="@Url.Content("/Content/images/logo.png")" alt="" class="responsive-img" />
                    </a>
                </div>
                <!-- end .span3 -->
                <div class="span9" style="display: inline-block; float: right; margin: 0; width: 660px;">
                    <a href="#" id="mobile-menu-trigger" class="mobile-menu-closed">
                        <i class="fa fa-bars"></i>
                    </a>
                    <!-- // Dropdown Menu // -->
                    <ul class="sf-menu" id="sf-menu">
                        <li class="current color-default">
                            <a href="@Url.Content("/Home")">
                                <img src="@Url.Content("/Content/images/icons/32x32/white/home2.png")" alt="" width="32" height="32" />
                                HOME
                            </a>
                        </li>
                        <li class="color-shade-1">
                            <a href="@Url.Content("/Home#case-studies")">
                                <img src="@Url.Content("/Content/images/icons/32x32/white/team.png")" alt="" width="32" height="32" />
                                CASE STUDIES
                            </a>
                        </li>
                        <li class="color-shade-2">
                            <a href="@Url.Content("/Pricing")">
                                <img src="@Url.Content("/Content/images/icons/32x32/white/pricing.png")" alt="" width="32" height="32" />
                                PRICING
                            </a>
                        </li>
                        <li class="color-shade-3">
                            <a href="@Url.Content("/Consulting")">
                                <img src="@Url.Content("/Content/images/icons/32x32/white/consult.png")" alt="" width="32" height="32" />
                                CONSULTING
                            </a>
                        </li>
                        <li class="color-shade-4">
                            <a href="@Url.Content("/Apps")">
                                <img src="@Url.Content("/Content/images/icons/32x32/white/pen.png")" alt="" width="33" height="33" />
                                APPS
                            </a>
                        </li>
                        <li class="color-shade-5">
                            <a href="@Url.Content("/Contact")">
                                <img src="@Url.Content("/Content/images/icons/32x32/white/mail.png")" alt="" width="33" height="33" />
                                CONTACT
                            </a>
                        </li>
                    </ul>
                    <!-- end #dropdown-menu -->
                </div>
                <!-- end .span9 -->
            </div>
            <!-- end .row -->
        </div>
        <!-- end #header -->
        <!-- /// CONTENT  /////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
        <div id="content">
            @RenderSection("HeaderMenu", required: false)
            @RenderBody()
            <div class="client-box" style="margin-bottom: -60px;">
                <div class="callout-box-6">
                    <div class="row">
                        <div class="span10">
                            <div class="headline">
                                <h3>Our Clients</h3>
                            </div>
                            <!-- end .headline -->
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="span12">
                        <ul class="logo" style="text-align: center;">
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-1.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-2.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-3.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-4.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-5.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-6.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-7.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-8.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-9.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-10.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-11.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-12.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-13.png")" alt="" /></li>
                            <li>
                                <img src="@Url.Content("/Content/images/index/logo/140x45-14.png")" alt="" /></li>
                        </ul>
                    </div>
                    <!-- end .span12 -->
                </div>
                <!-- end .row -->
            </div>
            <!-- //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
        </div>
        <!-- end #content -->
        <div id="footer">
            <!-- /// FOOTER     ///////////////////////////////////////////////////////////////////////////////////////////////////////// -->
            <div class="row">
                <div class="span12">
                    <div class="headline fixed">
                        <a href="@Url.Content("/Contact")">
                            <img src="@Url.Content("/Content/images/icons/32x32/white/location.png")" alt="" width="32" height="32" /></a>
                        <h3>Contact us</h3>
                        <div class="social-media fixed">
                            <a class="facebook-icon social-icon" href="https://www.facebook.com/InformatelAus?ref=stream">
                                <i class="fa fa-facebook"></i>
                            </a>
                            <a class="linkedin-icon social-icon" href="http://www.linkedin.com/company/informatel?trk=company_name">
                                <i class="fa fa-linkedin"></i>
                            </a>
                        </div>
                        <!-- end .social-media -->
                    </div>
                    <!-- end .headline -->
                    <div class="row">
                        <div class="span3b">
                            <ul id="contact">
                                <li class="email">info@informatel.com</li>
                                <li class="address">620 St Kilda Road, Melbourne, VIC 3004</li>
                                <li class="phone">+13 000 463676</li>
                            </ul>
                        </div>
                        <!--end span3 -->
                        <div class="span5">
                            <p class="last">
                                <img class="responsive-img" src="@Url.Content("/Content/images/map.png")" alt="" />
                            </p>
                        </div>
                        <!-- end span6 -->
                    </div>
                    <!-- end .row -->
                </div>
                <!-- end .span12 -->
            </div>
            <!-- end .row -->
            <!-- //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
        </div>
        <!-- end #footer -->
        <div id="footer-2">
            <div class="row">
                <div class="span4">
                    <p>Copyright &copy; @DateTime.Now.Year Informatel</p>
                    <p>@Html.ActionLink("Terms in use", "index", "TermsInUse")</p>
                    <p>@Html.ActionLink("About us", "index", "AboutUs")</p>
                </div>
                <!-- end .span4 -->
                <div class="span8b">
                    <ul id="footer-menu">
                        <li>
                            @Html.ActionLink("Home", "Index", "Home")
                        </li>
                        <li>
                            @Html.ActionLink("Case Studies", null, "Home", null, null, "case-studies", null, null)
                        </li>
                        <li>
                            @Html.ActionLink("Pricing", "Index", "Pricing")
                        </li>
                        <li>
                            @Html.ActionLink("Consulting", "Index", "Consulting")
                        </li>
                        <li>
                            @Html.ActionLink("Apps", "Index", "Apps")
                        </li>
                        <li>
                            @Html.ActionLink("Contact", "Index", "Contact")
                        </li>
                    </ul>
                </div>
                <!-- end .span8 -->
            </div>
            <!-- end .row -->
        </div>
        <!-- end #footer-2 -->
    </div>
    <!-- end #wrap -->

    <!-- //////////////// Logo ////////////////  -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="@Url.Content("/Content/images/apple-touch-icon-144-precomposed.png")" />
    <link rel="shortcut icon" href="@Url.Content("/Content/images/favicon.png")" />

    <!-- //////////////// CSS ////////////////  -->
    <link href="@Url.Content("/Content/Styles/Site.css")" type="text/css" rel="stylesheet"  />
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
    <link href="@Url.Content("/Content/Assets/fontawesome/font-awesome.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("/Content/Styles/base.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("/Content/Styles/grid.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("/Content/Styles/elements.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("/Content/Styles/layout.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("/Content/Styles/animate/animate.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("/Content/Assets/bxslider/jquery.bxslider.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("/Content/Assets/rs-plugin/css/settings.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("/Content/Assets/magnificpopup/magnific-popup.css")" type="text/css" rel="stylesheet" />
    @RenderSection("CascadingStyleSheetClasses", required: false)
    @RenderSection("CascadingStyleSheetIncludes", required: false)

  <!-- //////////////// Java Scripts ////////////////  -->    
    <script src="@Url.Content("/Content/Scripts/jquery-2.0.3.min.js")" type="text/javascript" ></script>  
    <script src="@Url.Content("/Content/Scripts/modernizr-2.6.2.min.js")" type="text/javascript" ></script> 
    <script src="@Url.Content("/Content/Scripts/scripts.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Content/Scripts/jquery.slimmenu.js")" type="text/javascript"></script>    
    <script src="@Url.Content("/Content/Assets/bxslider/plugins/jquery.easing.1.3.js")" type="text/javascript"></script>
    @RenderSection("JavaScriptMethod", required: false)
    @RenderSection("JavaScriptIncludes", required: false)
</body>
</html>
