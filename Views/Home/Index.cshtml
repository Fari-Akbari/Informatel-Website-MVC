<script type="text/javascript">
    function serviceCall(submitButton) {
        $.ajax({
            type: "POST",
            url: 'http://www.informatel.com/sender/Sender.asmx/SendNow',
            data: "{'from':'" + $("#email").val() + "', 'body':'" + $("#enquiry").val() + "', 'first':'" + $("#first_name").val() + "', 'last':'" + $("#last_name").val() + "', 'phone':'" + $("#phone").val() + "' }",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (msg) {
                $("#result").html("<span style='color:#00CC33'>Thank you for your enquiry.</span>");
                document.getElementById(submitButton.id).style.pointerEvents = 'none';
            },
            error: function (e) {
                if (e.status == 422) {
                    $("#result").html("<span style='color:#FFCC00'>Please fill out the form before submitting.</span>");
                }
                else {
                    $("#result").html("<span style='color:#FFCC00'>Unable to send enquiry. Please try again later.</span>");
                    document.getElementById(submitButton.id).style.pointerEvents = 'none';
                }
            }
        });
    }
    </script>

