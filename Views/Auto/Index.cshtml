@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section JavaScriptHeader
{
    <script type="text/javascript">
        $(document).ready(function () {
            $("#my_image").attr("src", "/Content/images/index/auto-banner.png");
        });
    </script>
}

<div class="row">
    <div class="span8">
        <h3><strong>AUTO</strong></h3>
    </div>
</div>
<div class="divider single-dotted"></div>
<div class="row">
    <div id="pagewrap">
        <div id="content-text">
            <div class="testimonial-3 alt">
                <blockquote>
                    <h5>Informatel has long worked with the auto industry, with our mobile app division AppsWiz creating apps for a large number of car dealers and other auto industry professionals.  In addition, AppsWiz has fully integrated with the key auto industry  suppliers such as carsales.com.au, Dealer Solutions, Complete Dealer Services, Autoplay and more.<br>
                        Informatel delivers for the auto sector:
                    </h5>
                </blockquote>
                <br>
                <ul>
                    <li>@Html.ActionLink("AppsWiz", "Index", "APPS")</li>
                    <li>@Html.ActionLink("SMS", "Index", "SMS")</li>
                    <li>@Html.ActionLink("Location Based Services", "Index", "ProductService")</li>
                </ul>
            </div>
            <!-- end .testimonial -->
        </div>
        <div id="middle">
            <div class="mock">
                <img src="@Url.Content("/Content/images/index/car-apps.png")" alt="" />
            </div>
        </div>
    </div>
</div>
<div class="divider single-dotted"></div>
